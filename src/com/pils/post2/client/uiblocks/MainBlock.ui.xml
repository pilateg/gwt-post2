<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:w='urn:import:com.pils.post2.client.layout.widgets'>
	<ui:with field="resource" type="com.pils.post2.client.layout.Resources"/>
	<g:FlowPanel>
		<g:FlowPanel ui:field="breadcrumbPanel" height="50px" styleName="{resource.css.floatPanel}"/>
		<w:Button ui:field="addEntryButton" text="add entry"/>
		<g:FlowPanel ui:field="contentPanel" styleName="{resource.css.block}"/>
		<g:HorizontalPanel styleName="{resource.css.navigationPanel}">
			<w:Button ui:field="firstButton" text="&lt;&lt;" styleName="{resource.css.leftButton}"/>
			<w:Button ui:field="previousButton" text="&lt;" styleName="{resource.css.leftButton}"/>
			<g:ScrollPanel>
				<g:FlowPanel ui:field="navigationPanel"/>
			</g:ScrollPanel>
			<w:Button ui:field="nextButton" text="&gt;" styleName="{resource.css.rightButton}"/>
			<w:Button ui:field="lastButton" text="&gt;&gt;" styleName="{resource.css.rightButton}"/>
		</g:HorizontalPanel>
	</g:FlowPanel>
</ui:UiBinder>